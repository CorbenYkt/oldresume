@prefix ind:<URN:inds:>.
@prefix prop:<URN:prop:>.
@prefix classes:<URN:class>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ind:process0 a classes:process;
    rdfs:label "Image Recognition";
    rdf:isDefinedBy	"Dmitrii Artemev";
    prop:hasKPI ind:rulesofrecognition;
    prop:hasKPI	ind:mlalgorithm;
    prop:hasInput ind:imagedatasets;
    prop:hasInput ind:Image;
    prop:hasResource ind:teammates;
    prop:hasResource ind:googlecloud;
    prop:hasOutput ind:Text.

ind:process01 prop:SubProcess ind:process0;
    rdfs:label "C1 and Image preparing";
    prop:hasInput ind:imagedatasets;
    prop:hasInput ind:Image;
    prop:hasResource ind:teammates;
    prop:hasOutput ind:process03;
    prop:hasOutput ind:process02.

ind:process02 prop:SubProcess ind:process0;
    rdfs:label "Acquire model";
    prop:hasInput ind:process01;
    prop:hasResource ind:teammates;
    prop:hasResource ind:googlecloud;
	prop:hasKPI ind:rulesofrecognition;
    prop:hasKPI ind:mlalgorith;
    prop:hasOutput ind:process03.

ind:process03 prop:SubProcess ind:process0;
	rdfs:label "Recognition";
    prop:hasInput ind:process01;
   	prop:hasKPI	ind:process02;
    prop:hasResource ind:teammates;
    prop:hasResource ind:googlecloud;
    prop:hasOutput ind:Text.

ind:process04 prop:SubProcess ind:process02;
	rdfs:label "Developing a model";
    prop:hasInput ind:process01;
    prop:hasKPI ind:rulesofrecognition;
    prop:hasKPI ind:mlalgorith;
    prop:hasResource ind:teammates;
    prop:hasResource ind:googlecloud;
    prop:hasOutput ind:process05.

ind:process05 prop:SubProcess ind:process04;
	rdfs:label "Tuned model";
    prop:hasInput ind:process01;
    prop:hasResource ind:teammates;
    prop:hasResource ind:googlecloud;
    prop:hasOutput ind:Text.